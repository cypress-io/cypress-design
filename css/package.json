{
  "name": "@cypress-design/css",
  "version": "0.13.2",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.mjs",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "require": "./package.json"
    },
    "./dist/colors": {
      "import": "./dist/colors.es.mjs",
      "require": "./dist/colors.cjs.js",
      "types": "./dist/colors.d.ts"
    },
    "./dist/icon-extractor-tools": {
      "import": "./dist/icon-extractor-tools.es.mjs",
      "require": "./dist/icon-extractor-tools.cjs.js",
      "types": "./dist/icon-extractor-tools.d.ts"
    },
    "./dist/theme.config": {
      "import": "./dist/theme.config.es.mjs",
      "require": "./dist/theme.config.cjs.js",
      "types": "./dist/theme.config.d.ts"
    }
  },
  "scripts": {
    "build": "yarn build:types && yarn build:lib",
    "build:types": "tsc -p ./tsconfig.build.json --noEmit false --emitDeclarationOnly",
    "build:lib": "rollup -c ./rollup.config.mjs"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "resolve-pkg": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/lodash": "^4.14.194",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.4",
    "color-string": "1.9.1"
  },
  "license": "MIT"
}