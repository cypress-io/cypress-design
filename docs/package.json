{
  "name": "@cypress-design/docs",
  "private": true,
  "version": "0.0.1",
  "files": [
    "*"
  ],
  "scripts": {
    "build": "node ../scripts/copy-md.mjs && vue-docgen -c ./docgen/docgen.config.cjs --logLevel=debug && yarn build:components && ./vercel-build-docs.sh",
    "docgen:watch": "vue-docgen --watch -c ./docgen/docgen.config.cjs",
    "preview": "vite preview",
    "dev": "rimraf docs/components && NODE_ENV=development yarn vite dev"
  },
  "dependencies": {
    "@cypress-design/vue-tooltip": "*",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@vueuse/head": "^1.3.1",
    "concurrently": "^8.2.1",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "react-docgen-typescript": "^2.2.2",
    "shiki": "^0.14.3",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-markdown": "^0.23.7",
    "vite-ssg": "^0.23.1",
    "vite-ssg-sitemap": "^0.5.1",
    "vitest": "^0.34.3",
    "vue": "3.3.4",
    "vue-component-meta": "^1.8.8",
    "vue-docgen-api": "^4.74.0",
    "vue-docgen-cli": "^4.67.0",
    "vue-live": "^2.5.4",
    "vue-router": "^4.2.4"
  },
  "license": "MIT"
}