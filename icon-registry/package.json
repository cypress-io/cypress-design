{
  "name": "@cypress-design/icon-registry",
  "version": "0.0.0",
  "private": true,
  "description": "All svg files to be exposed to the Icon component",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:types": "node ./build-types.mjs",
    "build:svg": "svg-to-ts-files",
    "build": "yarn build:types && yarn build:svg"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@cypress-design/css": "^0.1.0",
    "camelcase": "^6.3.0",
    "dedent": "^0.7.0",
    "globby": "^13.1.1",
    "svg-to-ts": "^8.6.1"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./icons/*.svg"
    ],
    "outputDirectory": "./dist",
    "interfaceName": "CyIcon",
    "typeName": "cyIcon",
    "barrelFileName": "iconsList",
    "prefix": "",
    "modelFileName": "cyIcon.model",
    "compileSources": true,
    "iconsFolderName": "icons"
  }
}