{
  "name": "@cypress-design/icon-registry",
  "version": "0.4.1",
  "description": "All svg files to be exposed to the Icon component",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build:lib": "tsc -p tsconfig.json",
    "build:libcjs": "tsc -p tsconfig.json --module commonjs --outDir \"dist/cjs\" --declaration false --declarationMap false --declarationDir null",
    "build:icons": "node ./build-icons.mjs",
    "build:svg": "svg-to-ts-files",
    "build": "yarn build:icons && yarn build:svg && yarn build:lib && yarn build:libcjs"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "camelcase": "^6.3.0"
  },
  "devDependencies": {
    "@cypress-design/css": "^0.5.1",
    "dedent": "^0.7.0",
    "globby": "^13.1.1",
    "svg-to-ts": "^8.6.1"
  },
  "svg-to-ts": {
    "srcFiles": [
      "./icons/*.svg"
    ],
    "interfaceName": "CyIcon",
    "outputDirectory": "./src",
    "typeName": "cyIcon",
    "barrelFileName": "iconsList",
    "prefix": "cy",
    "modelFileName": "icon.model",
    "iconsFolderName": "iconsData",
    "completeIconSetName": "iconSet",
    "exportCompleteIconSet": true,
    "delimiter": "CAMEL"
  }
}
